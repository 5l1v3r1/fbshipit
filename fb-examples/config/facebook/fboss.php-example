namespace Facebook\ShipIt\Config;

use type Facebook\ShipIt\{FBShipItCLIStaticConfig, FBShipItConfig};

final class FacebookFboss extends FBShipItConfig {

  <<__Override>>
  protected static function getDefaultStrippedFiles(): ImmVector<string> {
    return ImmVector {
      '@.*facebook.*@',
      '@fbcode/fboss/agent/hw/bf.*@',
      '@fbcode/fboss/agent/platforms/bf_wedge.*@',
      '@fbcode/fboss/THIRD-PARTY-VERSIONS@',
      '@.*tphacks.*@',
      '@fbcode/fboss/README.md@',
    };
  }

  <<__Override>>
  public static function getDefaultPathMappings(): ImmMap<string, string> {
    return ImmMap {
      'fbcode/fboss/github/' => '',
      'fbcode/fboss/common/' => 'common/',
      'fbcode/fboss/' => 'fboss/',
    };
  }

  <<__Override>>
  public static function getStaticConfig(): FBShipItCLIStaticConfig {
    return shape(
      'internalRepo' => 'fbsource',
      'githubOrg' => 'facebook',
      'githubProject' => 'fboss',
    );
  }
}
